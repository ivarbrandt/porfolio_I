      <h1 class="display-3 text-center">Contact Me!</h1>
      <div class="form-group" style="max-width: 30rem;">
        <form action="/" method="POST">
          Name<input type="text" class="form-control" name="first_name" />
          Email<input type="text" class="form-control" name="last_name" />
          Message<input
            name="message"
            type="text"
            class="form-control rows:3"
          />
          <a href="/" class="btn btn-primary" style="margin-top:1rem">Send</a>
        </form>
      </div>
    </div>
